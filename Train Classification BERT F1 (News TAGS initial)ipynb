        "### Show training results"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "VeC6yyxl5L6C"
      },
      "source": [
        "def show_train_results(train_history, val_history, img_name='graph'):\n",
        "    fig, ax = plt.subplots(figsize=(7, 5))\n",
        "    ax.plot(train_history, label='train f1')\n",
        "    ax.plot(val_history, label='val f1')\n",
        "    ax.set_title('Training F1-score')\n",
        "    ax.set_xlabel('Epoch number')\n",
        "    ax.set_ylabel('F1-score')\n",
        "    ax.legend(loc='best')\n",
        "    ax.set_ylim([0, 1.1]);\n",
        "    plt.savefig(img_name, dpi=300)"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
